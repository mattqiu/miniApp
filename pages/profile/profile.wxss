.container{
    background: #f4f4f4;
}
.container:before{
        background: #fff;
}
.nav{
    padding: 0 23.4rpx;
    height: 70.2rpx;
    border-bottom: 1px solid #ddd;
    display: flex;
    color: #222;
    font-size: 15px;
    background-color: #fff;
}
.nav-item{
    padding: 0 46.8rpx;
    width: 187.2rpx;
    line-height: 70.2rpx;
}
.nav-item.active {
    margin-bottom: -1px;
    color: #00bebf;
    border-bottom: 4px solid #4ed6d1;
    z-index: 10;
}
.more{
    padding-left: 35.1rpx;
}
.term{
    height: 117rpx;
    line-height: 117rpx;
    border-bottom: 1px solid #d9d9d9;
    display: flex;
    justify-content: space-between;
    color: #222;
}
.term-key{
    font-size: 14px;
}
.term-value{
    margin-right: 46.8rpx;
    font-size: 13px;
}
.term-user-img{
    margin-top: 25.74rpx;
    width: 65.52rpx;
    height: 65.52rpx;
    background-color: #c5c5c5;
    border-radius: 50%;
}
.empty{
    padding-top: 50%;
    width: 100%;
    height: 100%;
    text-align: center;
}
.empty text{
   font-size: 16px;
   color: #777;
}
.adress-list{
    margin-top: 11.7rpx;
    padding: 0 23.4rpx;
}
.list-box{
    margin-top: 23.4rpx;
    padding: 23.4rpx 0 28.08rpx 35.1rpx;
    border: 2px solid #ddd;
    display: flex;
}
.list-box.active{
    border: 2px solid #4ed7d0;
}
.list-cont{
    padding: 0 35.1rpx 0 0;
    width: 558.09rpx;
    border-right: 1px solid #ddd;
}
.cont-contact{
    padding: 9.36rpx 0 7.02rpx;
    position: relative;
}
.cont-ad, .cont-ad-desic{
    font-size: 12px;
    color: #777;
    line-height: 18px;
}
.cont-contact .name,
.cont-contact .mobile{
    margin-right: 23.4rpx;
}
.cont-contact .name{
    font-size: 15px;
    color: #222;
}
.cont-contact .mobile{
    font-size: 14px;
    color: #222;
}
.cont-contact .default{
    position: absolute;
    right: 0;
    top: 9.36rpx;
    width: 70.2rpx;
    height: 35.1rpx;
    background-color: #4ed7d0;
    border-radius: 3px;
    text-align: center;
    line-height: 35.1rpx;
    color: #fff;
    font-size: 12px;
}
.cont-contact .cont-ad-desic{
    font-size: 12px;
    color: #777;
    line-height: 36px;
}

.addAdress-btn{
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 111.15rpx;
    background-color: #4ed6d2;
    color: #fff;
    font-size: 18px;
    line-height: 111.15rpx;
    text-align: center;
}
.order-list{
    margin-top: 22.23rpx;
    padding-left: 35.1rpx;
    background-color: #fff;
    border-bottom: 1px solid #ddd;
}
.order-state{
    padding-right: 35.1rpx;
    height: 93.6rpx;
    line-height: 93.6rpx;
    border-bottom: 1px solid #ddd;
    display: flex;
    justify-content: space-between;
}
.order-pro{
    margin-bottom: -1px;
    padding-right: 35.1rpx;
    border-bottom: 1px solid #ddd;
}
.pro-box{
    padding: 28.08rpx 0;
    display: flex;
    border-bottom: 1px solid #ddd;
}
.order-pro .pro-box:last-child{
    border-bottom: 0;
}
.pro-img{
    margin-right: 35.1rpx;
    width: 154.44rpx;
    height: 154.44rpx;
    border: 1px solid #ddd;
}
.pro-desic{
    padding: 1px 0;
    display: flex;
    flex-direction: column;
}
.pro-desic .title{
    font-size: 13px;
    color: #222;
    line-height: 39.78rpx;
}
.pro-order{
    height: 105.3rpx;
    line-height: 105.3rpx;
    display: flex;
}