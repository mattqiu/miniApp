@font-face {
	font-family: 'xidi';
	src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
	font-weight: normal;
	font-style: normal;
}

.xd {
  font-family: "xidi";
  display: inline-block;
  vertical-align: middle;
  line-height: 1;
  font-weight: normal;
  font-style: normal;
  speak: none;
  text-decoration: inherit;
  text-transform: none;
  text-rendering: auto;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

