
<!--更多设置-->
<view class="more">
  <view class="term">
    <text class="term-key">用户头像</text>
    <view class="term-value term-user-img">
      <image src="{{more.avatarUrl}}"></image>
    </view>
  </view>
  <view class="term">
    <text class="term-key">用户昵称</text>
    <text class="term-value">{{more.nickName}}</text>
  </view>
  <navigator url="/pages/profile/more/question/question">
    <view class="term">
      <text class="term-key">常见问题</text>
      <view class="term-value">
        <text class="xd xd-right-angle"></text>
      </view>
    </view>
  </navigator>
</view>