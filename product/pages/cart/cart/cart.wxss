.container.has-goods{
  /*background: #f4f4f4;*/
  padding-bottom: 114.66rpx; 
}

.free-shipping-tips {
  width: 100%;
  height: 77.22rpx;
  line-height: 77.22rpx;
  background-color: #faf6eb;
  font-size: 28.08rpx;
  color: #ff7765;
  text-indent: 15px;
}
/*商品列表*/
@import './wxss/list.wxss';

/*失效商品*/
.invalid-goods{
  width: 100%;
  height: 187.2rpx;
  background: #fff;
}

.invalid-goods-navigator{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 0 23.4rpx;
  height: 93.6rpx;
  border-bottom: 2rpx solid #d9d9d9;
  font-size: 28.08rpx;
  color:#222;
}

.invalid-goods-navigator .xd{
  font-size: 32.76rpx;
  color: #c5c5c5;
}

/*结算*/
@import './wxss/settlement.wxss';

/*购物车为空*/

.cart-null {
  text-align: center;
}

.cart-null image {
  margin-top: 249.21rpx;
  width: 252.72rpx;
  height: 224.64rpx;
}

.cart-null .null-tip {
  margin-top: 77.22rpx;
  font-size: 32.76rpx;
  line-height: 1;
  color: #222;
}

.cart-null .null-fun {
  margin-top: 25.74rpx;
  font-size: 28.08rpx;
  line-height: 1;
  color: #777;
}

.cart-null .go-index {
  margin: 77.22rpx auto 0;
  width: 643.5rpx;
  height: 105.3rpx;
  line-height: 105.3rpx;
  text-align: center;
  border: 1px solid #ddd;
  font-size: 42.12rpx;
  color: #222;
}
